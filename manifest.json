{
	"manifest_version": 2,
	"name": "MyGext",
	"description": "Youtube playlist player",
	"version": "1.0",
	"author": "Aakash Kumar",
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Youtube playlist player"
	},
	"background": {
		"scripts": ["aakashkumar1997.github.io/popup.js"],
		"permissions": [ "https://www.aakashkumar1997.github.io/*" ],
		"persistent": true
	},
	"permissions": [
		"activeTab"
	]
}